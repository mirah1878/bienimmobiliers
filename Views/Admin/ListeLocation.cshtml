@{
    Layout = "~/Views/Shared/Admin.cshtml";
}


<section class="section">
    <div class="row">
        <div class="col-lg-12">
        <div class="card-body">
          <div class="card">
            <div class="card-body">
                <table class="table table-bordered border-primary">
                    <thead>
                      <tr>
                        <th scope="col">Bien</th>
                        <th scope="col">idclient</th>
                        <th scope="col">duree</th>
                        <th scope="col">date debut </th>
                      </tr>
                    </thead>
                    <tbody>
                    @foreach(var list in ViewBag.list){
                      <tr>
                        <td><a asp-action="VoirDetail" asp-controller="Admin" asp-route-idlocation="@list.Id">@list.IdBien</a></td>
                        <td>@list.IdClient</td>
                        <td>@list.Duree</td>
                        <td>@list.DateDebut</td>
                      </tr>
                    }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</section>
